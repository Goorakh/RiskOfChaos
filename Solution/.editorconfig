[*.cs]

# CS0649: Field 'GiveRandomItem._effectInfo' is never assigned to, and will always have its default value
dotnet_diagnostic.CS0649.severity = none

# CS0067: The event 'DisableProcs.OnValueDirty' is never used
dotnet_diagnostic.CS0067.severity = suggestion

# IDE0046: Convert to conditional expression
dotnet_style_prefer_conditional_expression_over_return = false

# IDE0051: Remove unused private members
dotnet_diagnostic.IDE0051.severity = none

# IDE0045: Convert to conditional expression
dotnet_style_prefer_conditional_expression_over_assignment = false

# IDE0290: Use primary constructor
csharp_style_prefer_primary_constructors = false

# IDE0066: Convert switch statement to expression
csharp_style_prefer_switch_expression = false

# IDE0016: Use 'throw' expression
csharp_style_throw_expression = false

# IDE0040: Add accessibility modifiers
dotnet_style_require_accessibility_modifiers = omit_if_default

# IDE1006: Naming Styles
dotnet_diagnostic.IDE1006.severity = none
